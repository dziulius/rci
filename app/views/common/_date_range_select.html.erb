<% if object.start_at and object.end_at %>
    <% form_tag send(url, object, :format => 'js'), :id => "period_select" do %>
        <%= compact_month_select :date_from, object.start_at, object.start_at, object.end_at %>
        <%= compact_month_select :date_to, object.end_at, object.start_at, object.end_at %>
    <% end %>

    <script type="text/javascript">
        $(function() {
            $('#date_from, #date_to').selectToUISlider({sliderOptions:{
                change: function() {
                    $('#users').load($('#period_select').attr('action'), $.param($($('#period_select')).serializeArray()));
                }
            }});
        });
    </script>
<% end %>